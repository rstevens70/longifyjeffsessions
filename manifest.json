{
"manifest_version": 2,
    "name": "Longify Jeff Sessions",
    "short_name": "Longify Jeff",
    "description": "Changes all mentions of Jeff Sessions to Jefferson Beauregard Sessions III, Esquire.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}